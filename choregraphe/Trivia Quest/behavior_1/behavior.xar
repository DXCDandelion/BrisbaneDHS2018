                                    <![CDATA[import csv, random
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        self.memory = ALProxy("ALMemory")
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        isTrueFalse = 1
        #bring in Category questions array from memory
        questionList = self.memory.getData("categoryQuestions")

        chosenQuestion = random.randint(0, len(questionList) - 1)
        self.memory.insertData("correctAnswer",questionList[chosenQuestion][1][0])
        self.memory.insertData("currentQuestion",questionList[chosenQuestion][0])

        #Shuffle the choices order
        choicesShuffled = questionList[chosenQuestion][1]
        #self.logger.info(choicesShuffled)
        random.shuffle(choicesShuffled)
        #self.logger.info(choicesShuffled)

        #Save shuffles choices for later
        self.memory.insertData("chosenQuestionChoice01",choicesShuffled[0])#Choice_01
        self.memory.insertData("chosenQuestionChoice02",choicesShuffled[1])#Choice_02
        self.memory.insertData("chosenQuestionChoice03",choicesShuffled[2])#Choice_03

        if (choicesShuffled[0] == "blank"):
            choicesShuffled[0] = choicesShuffled[2]
            self.memory.insertData("chosenQuestionChoice01",choicesShuffled[0])#Choice_01
            self.memory.insertData("isTrueFalse",isTrueFalse)
        elif(choicesShuffled[1] == "blank"):
            choicesShuffled[1] = choicesShuffled[2]
            self.memory.insertData("chosenQuestionChoice02",choicesShuffled[1])#Choice_02
            self.memory.insertData("isTrueFalse",isTrueFalse)


        #self.logger.info(chosenQuestionChoice01)
        #self.logger.info(chosenQuestionChoice02)
        #self.logger.info(chosenQuestionChoice03)


        #Remove chosen Q from list so it's not asked multiple times
        del questionList[chosenQuestion]
        #Re-insert catergory questions
        self.memory.insertData("categoryQuestions", questionList)
        self.onStopped() #activate the output of the box

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
