                                    <![CDATA[# /!\ Generated content. Do not edit!
class MyClass(GeneratedClass):
	def __init__(self):
		try: # disable autoBind
		  GeneratedClass.__init__(self, False)
		except TypeError: # if NAOqi < 1.14
		  GeneratedClass.__init__( self )

	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("touch head")):
			self.output_1(p)
		elif(p == self.typeConversion("touch hand")):
			self.output_2(p)
		elif(p == self.typeConversion("touch chest")):
			self.output_3(p)
		elif(p == self.typeConversion("raise both arms")):
			self.output_4(p)
		elif(p == self.typeConversion("raise one arm")):
			self.output_5(p)
		elif(p == self.typeConversion("touch knee")):
			self.output_6(p)
		elif(p == self.typeConversion("hold one arm forward")):
			self.output_7(p)
		elif(p == self.typeConversion("hold both arms forward")):
			self.output_8(p)
		elif(p == self.typeConversion("hold one arm out")):
			self.output_9(p)
		elif(p == self.typeConversion("hold both arms out")):
			self.output_10(p)
		elif(p == self.typeConversion("dab")):
			self.output_11(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
