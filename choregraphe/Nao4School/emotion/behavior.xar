	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("faces")):
			self.output_1(p)
		elif(p == self.typeConversion("cards")):
			self.output_2(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
